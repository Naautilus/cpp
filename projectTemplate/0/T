FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      v2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    inlet
    {
		type			timeVaryingMappedFixedValue;
        //type            zeroGradient;
	}
    sides
    {
        type            zeroGradient;
    }
    outlet
    {
        type            freestream;
        freestreamValue uniform 300;
    }
}


